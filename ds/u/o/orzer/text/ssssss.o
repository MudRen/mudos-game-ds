                 else status2=\"\";                            if ((in_edit(user_list[i])))                              status3=sprintf(HIY\"([½s¿è]: %s)\"NOR,in_edit(user_list[i]));                              else status3=\"\";                           if (in_input(user_list[i])) status4=HIC\"<¿é¤J>\"NOR;                              else status4=\"\";                           if(user_list[i]->query_temp(\"netdead\")) status5=HIB\"(Â_½u)\"NOR;                              else status5=\"\";                           write(sprintf(\"¡·%12s \"WHT+\"%10s\"NOR+\"(%10s) ¥¿¦b %s(%s)%s%s%s%s%s\\n\",SECURITY_D->get_status(user_list[i]),                           user_list[i]->query(\"name\"),                           user_list[i]->query(\"id\"),                            (string)where->query(\"short\"),                           (string)file_name(where),                           status1,status2,status3,status4,status5)); //                   if ((status1!=\"\")||(status2!=\"\")||(status3!=\"\")||(status4!=\"\")||(status5!=\"\"))//                   write(sprintf(\"ùø                    ª¬ºA: %s%s%s%s%s\\n\",status1,status2,status3,status4,status5));                     j++;                   }           }           write(\"¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w\\n\");           this_player()->start_more(sprintf(\"¥Ø«e¦@¦³ %d ¦ìwiz¦b½u¤W.\\n\",j-1));           return 1;   }//---------------------------------|Show Players|---------------------------------int do_query_players(string arg)  {      int i,j=1,n,idle;      object *user_list,where,current_ppl;      string club,status1,status2,status3,status4,status5;      user_list = users();      n = sizeof(user_list);      write(HIG+this_object()->name()+HIW\"¦C¥X¤F½u¤WPlayers¦W³æ:\\n\"NOR);      write(\"¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w¢w\\n\");           for( i=0 ;i<n ;i++)          