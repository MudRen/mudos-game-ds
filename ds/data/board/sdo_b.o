                       if( score[x-1] > score[x] )                        {                                swap=score[x-1];                                score[x-1]=score[x];                                score[x]=swap;                                user=ppl[x-1];                                ppl[x-1]=ppl[x];                                ppl[x]=user;                        }                }        }        i=sizeof(ppl);        j=0;        msg= HIW\"--==--==--==--==--==--==--==--==--==--==--==--==--==--==--==--==--==--==--==--==--==--\\n\"NOR;        msg += HIG\"\\t\\t\\t°ï³·¤H¤jÁÉ°ª¤â±Æ¦æº]\\n\";        msg+=HIW\"--==--==--==--==--==--==--==--==--==--==--==--==--==--==--==--==--==--==--==--==--==--\\n\"NOR;        while( i-- )        {                j++;                if( ppl[i]==getuid(this_player()) ) msg+= HIC;                        else msg += CYN;                if( game_record[ppl[i]] > 0 ) {                        msg+=sprintf(\"²Ä%4s¦W ---- %13s ¥Ø«e°ª«× %3f ¤½¤À\",                                chinese_number(j),ppl[i],game_record[ppl[i]]);                        switch( j )                        {                                case 1: msg+= HIM\"\"BLINK\" (¶W¯Å¤j¼ú¡G¤W¦W¤H°ó¡®¦Û­q Save Eq ¤@¥ó)\\n\"; break;                                case 2..5: msg+= HIY\" (¤j¼ú¡GÀH¾÷ Save Eq ¤@¥ó)\\n\"; break;                                default: msg+= HIR\" (°Ñ¥[¼ú¡G¿ú¡BExp)\\n\"; break;                                }                }                msg+=NOR;        }               msg+=HIW\"--==--==--==--==--==--==--==--==--==--==--==--==--==--==--==--==--==--==--==--==--==--\\n\"NOR;        return msg;}int measure(object me){        int roll1,roll2,comp1,comp2;        float temp,high,temp1,temp2;        string id;        id=getuid(me);                if( !restore() ) game_record=([]);                if( !mapp(game_record) ) game_record=([]);                temp=game_record[id];               